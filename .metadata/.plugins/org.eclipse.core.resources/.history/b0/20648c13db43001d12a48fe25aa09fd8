package com.miBanco.domain;

public class prueba {

	public static void main(String[] args) {
		Cliente c = new Cliente("Jorge", "Asensio");
		System.out.println("Creando al cliente "+ c.getNombre() +" "+ c.getApellido());
		Cuenta cuenta1= new Cuenta(500);
		System.out.println("Creando su cuenta con saldo: " + cuenta1.getBalance() );
		System.out.println("Ingresar 22.50: "+ cuenta1.depositar(22.50));
		System.out.println("Retira 400.00: " + cuenta1.retirar(400));
		System.out.println("El cliente "+ c.getNombre() +" "+ c.getApellido()+ " tiene un balance de : "+ cuenta1.getBalance());

	}

}
